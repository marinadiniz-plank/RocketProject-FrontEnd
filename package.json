{
	"name": "frontend",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "vite build",
		"lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
		"preview": "vite preview",
		"format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\"",
		"storybook": "storybook dev -p 6006",
		"build-storybook": "storybook build",
		"cypress:dev": "concurrently --kill-others --success first \"vite preview\" \"cypress run --env baseUrl=\"http://localhost:4173\"\" || exit 0",
		"cypress:prod": "npm run cypress:run -- --env baseUrl=\"https://rocketproject-frontend-marinadiniz-plank.vercel.app/\" --spec \"cypress/e2e/**/*.cy.tsx\"",
		"test": "jest"
	},
